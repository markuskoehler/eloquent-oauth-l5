{
  "name": "markuskoehler/eloquent-oauth-l5",
  "description": "Stupid simple OAuth authentication with Laravel and Eloquent",
  "authors": [
    {
      "name": "Adam Wathan",
      "email": "adam.wathan@gmail.com"
    },
    {
      "name": "Markus Koehler",
      "email": "markus.koehler@rockwellcollins.com",
      "role": "Software Engineer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.5.0",
    "markuskoehler/eloquent-oauth": "dev-master",
    "guzzlehttp/guzzle": "^6.2.1"
  },
  "autoload": {
    "psr-4": {
      "AdamWathan\\EloquentOAuthL5\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdamWathan\\EloquentOAuthL5\\EloquentOAuthServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/markuskoehler/eloquent-oauth.git"
    }
  ]
}
